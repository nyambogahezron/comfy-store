{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"outputs": [".next/**", "!.next/cache/**", "dist/**", "build/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"start": {
			"cache": false,
			"persistent": true
		},
		"lint": {},
		"format": {},
		"check": {},
		"type-check": {},
		"test": {},
		"clean": {
			"cache": false
		}
	}
}
